{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "Book",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "author": {
      "type": "string",
      "required": true
    },
    "pages": {
      "type": "integer",
      "required": true
    },
    "publicationDate": {
      "type": "date",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "ratings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::rating.rating",
      "mappedBy": "book"
    },
    "wishlists": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::wishlist.wishlist",
      "mappedBy": "book"
    }
  }
}
